-- åˆ›å»ºæ ‡ç­¾é¡µå‡½æ•°
local function CreateTab(section, title, icon)
    return section:Tab({Title = title, Icon = icon})
end

-- åˆ›å»ºæŒ‰é’®å‡½æ•°
local function CreateButton(tab, title, desc, callback)
    return tab:Button({
        Title = title,
        Desc = desc,
        Callback = callback
    })
end

-- åˆ›å»ºå¼€å…³å‡½æ•°
local function CreateToggle(tab, title, desc, value, callback)
    return tab:Toggle({
        Title = title,
        Desc = desc,
        Value = value,
        Callback = callback
    })
end

-- åˆ›å»ºæ»‘å—å‡½æ•°
local function CreateSlider(tab, title, desc, min, max, default, callback)
    return tab:Slider({
        Title = title,
        Desc = desc,
        Step = 1,
        Value = {Min = min, Max = max, Default = default},
        Callback = callback
    })
end

-- åˆ›å»ºé¢œè‰²é€‰æ‹©å™¨å‡½æ•°
local function CreateColorpicker(tab, title, desc, default, callback)
    return tab:Colorpicker({
        Title = title,
        Desc = desc,
        Default = default,
        Callback = callback
    })
end

