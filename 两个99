                    CreateEsp(Obj, Color3.fromRGB(255, 0, 0), Obj.Name, Obj.PrimaryPart, 2)
                end
            end
            task.wait(0.1)
        end
        for _, Obj in pairs(game.Workspace.Characters:GetChildren()) do
            KeepEsp(Obj, Obj.PrimaryPart)
        end
    end)
end)

